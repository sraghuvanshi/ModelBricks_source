                            </div>
                        </div>
                    </td>
                </tr>
                {{/each}}
                {{/each}}
                {{/each}}
                {{/each}}
                {{/each}}
            </tbody>
        </table>
    </div>
</div>

{{!-- Observables table  --}}
<div class="pmTable">
    <div class="container">
        <table class="responsive-table">
            <caption>Observables</caption>
            <thead>
                <tr>
                    <th scope="col">Name</th>
                    <th scope="col">Type</th>
                    <th scope="col">BioNetGen Defination</th>
                    <th scope="col">Count</th>
                </tr>
            </thead>
            <tbody>
                {{#each data.vcml.BioModel}}
                {{#each this.Model}}
                {{#each this.RbmModelContainer}}
                {{#each this.RbmObservableList}}
                {{#each this.Observable}}

                <tr>
                    <td data-title="Name">{{this.$.Name}}</td>
                    <td data-title="Domain">{{this.$.ObservableType}}</td>

                    {{#each this.SpeciesPattern}}
                    {{#each this.MolecularTypePattern}}
                    {{#each this.MolecularType}}
                    <td data-title="MolecularType">{{this.$.Name}}</td>
                    {{/each}}
                    {{/each}}
                    {{/each}}
