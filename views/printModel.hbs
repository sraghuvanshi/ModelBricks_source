<div class="sectionDescription">
    {{#each data.vcml.BioModel}}
    {{#each this.Version}}
    <span class="dashboardBtn"><a href="/curatedList/model/{{this.$.Name}}#dashboard"><i class="fa fa-undo"
                aria-hidden="true"></i> Dashboard</a></span>
    <h2>Name - <span>{{trimString this.$.Name}}</span></h2>
    <h2>BioModel ID - <span>{{this.$.KeyValue}}</span></h2>
    {{/each}}
    {{/each}}
</div>

<div class=" rxnDiagram">
    <h2 class="sectionTitle">Reaction Diagram</h2>
    {{#each data.vcml.BioModel}}
    {{#each this.Version}}
    <img class="image-zoom-available" src="https://vcellapi-beta.cam.uchc.edu:8080/biomodel/{{this.$.KeyValue}}/diagram"
        alt="reactionDiagram" draggable="false" />
    {{/each}}
    {{/each}}
</div>

<h2 class="sectionTitle">Physiology</h2>
{{!-- reactions table  --}}
<div class="pmTable">
    <div class="container">
        <table class="responsive-table">
            <caption>Reactions</caption>
            <thead>
                <tr>
                    <th scope="col">Reaction</th>
                    <th scope="col">Name</th>
                    <th scope="col">Structure</th>
                    <th scope="col">Kinetics</th>
                </tr>
            </thead>
            <tbody>

                {{#each data.vcml.BioModel}}
                {{#each this.Model}}
                {{#each this.SimpleReaction}}
                <tr>
                    <td data-title="Reaction">
                        {{#each this.Reactant}}
                        {{this.$.LocalizedCompoundRef}}
                        {{/each}}
                        <span style="color:#666666">--></span>
                        {{#each this.Product}}
                        {{this.$.LocalizedCompoundRef}}
                        {{/each}}
                    </td>
                    <td data-title="Name">{{this.$.Name}}</td>
                    <td data-title="Structure"> {{this.$.Structure}}</td>
                    <td data-title="Kinetics">
                        {{#each this.Kinetics}}
                        {{this.$.KineticsType}}
                        {{/each}}
                    </td>

                </tr>
                <tr>
                    <td class="printAnnotationWrap">
                        <div class="printAnnotation">

                            {{!-- universal ID's  --}}
                            <div class="printUID" id="reactionUID-{{this.$.Name}}">
                                <p class="heading">Universal ID's: </p>
                            </div>
                        </div>
                    </td>

                    <td class="printAnnotationWrap">
                        <div class="printAnnotation">

                            {{!-- annotation --}}
                            <div class="printTA" id="reactionTA-{{this.$.Name}}">
                                <p class="heading"> Text Annotations: </p>
                            </div>
                        </div>
                    </td>
                    {{#each this.Kinetics}}
                    {{#each this.Parameter}}
                    <td class="printAnnotationWrap">
                        <div class="printAnnotation">

                            {{!-- Kinetics --}}
                            <div class="printTA">
                                <p class="heading"> Reaction rate: {{this._}} <span
                                        class="unit">[{{this.$.Unit}}]</span></p>
                            </div>
                        </div>
                    </td>
                    {{/each}}
                    {{/each}}

                </tr>
                {{/each}}
                {{/each}}
                {{/each}}

            </tbody>
        </table>
    </div>
</div>

{{!-- Structures table  --}}
<div class="pmTable">
    <div class="container">
        <table class="responsive-table">
            <caption>Structures</caption>
            <thead>
                <tr>
                    <th scope="col">Name</th>
                    <th scope="col">Type</th>
                    <th scope="col">Electrical (Membrane Polarity)</th>
                </tr>
            </thead>
            <tbody>
                {{#each data.vcml.BioModel}}
                {{#each this.Model}}
                {{#each this.Membrane}}
                <tr>
                    <td data-title="Name">{{this.$.Name}}</td>
                    <td data-title="Type">Membrane</td>
                    <td data-title="Electrical">{{this.$.PositiveFeature}}(+) &nbsp; {{this.$.NegativeFeature}}(-)</td>
                </tr>
                <tr>
                    <td class="printAnnotationWrap">
                        <div class="printAnnotation">

                            {{!-- universal ID's  --}}
                            <div class="printUID" id="structureUID-{{this.$.Name}}">
                                <p class="heading">Universal ID's: </p>
                            </div>
                        </div>
                    </td>
                    <td class="printAnnotationWrap">
                        <div class="printAnnotation">

                            {{!-- annotation --}}
                            <div class="printTA" id="structureTA-{{this.$.Name}}">
                                <p class="heading"> Text Annotations: </p>
                            </div>
                        </div>
                    </td>
                </tr>
                {{/each}}
                {{/each}}
                {{/each}}
                {{#each data.vcml.BioModel}}
                {{#each this.Model}}
                {{#each this.Feature}}
                <tr>
                    <td data-title="Name">{{this.$.Name}}</td>
                    <td data-title="Type">Compartment</td>
                    <td data-title="Electrical">{{this.$.PositiveFeature}}(+) &nbsp; {{this.$.NegativeFeature}}(-)</td>
                </tr>
                <tr>
                    <td class="printAnnotationWrap">
                        <div class="printAnnotation">

                            {{!-- universal ID's  --}}
                            <div class="printUID" id="structureUID-{{this.$.Name}}">
                                <p class="heading">Universal ID's: </p>
                            </div>
                        </div>
                    </td>
                    <td class="printAnnotationWrap">
                        <div class="printAnnotation">

                            {{!-- annotation --}}
                            <div class="printTA" id="structureTA-{{this.$.Name}}">
                                <p class="heading"> Text Annotations: </p>
                            </div>
                        </div>
                    </td>
                </tr>
                {{/each}}
                {{/each}}
                {{/each}}
            </tbody>
        </table>
    </div>
</div>


