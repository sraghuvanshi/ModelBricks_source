                <tr>
                    <td data-title="Name">{{this.$.Name}}</td>

                    <td data-title="Description">
                        {{#each this.MolecularComponent}}
                        {{this.$.Name}}
                        {{#if this.AllowableState}}
                        {{#each this.AllowableState}}
                        ~{{this.$.Name}}
